#Add an item
a=(4,6,2,8,3,1)
print(a)
b=(4,6,2,8,3,1,9)
print(b)
c=a[0:5]+(15,20,25)+b[0:5]
print(c)
d=list(c)
d.append(30)
e=tuple(d)
print(e)

#Length of a tuple
a=input()
a=tuple(a)
print(a)
print(len(a))

#Reverse a tuple
a=input()
print(tuple(a[::-1]))

#Repeated element in a tuple
tp=(2,4,5,6,2,3,4,4,7)
print(tp)
n=int(input())
print(tp.count(n))

