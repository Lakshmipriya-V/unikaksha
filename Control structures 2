#Series 1
n=int(input())
a=1
for i in range(1,n+1):
    a=i**2
    print(a,end=" ")
    
#Series 2
n=int(input())
a=6
for i in range(1,n+1):
    print(a,end=" ")
    a=a+(5*i)
    
#Series 3
n=int(input())
a=3
for i in range(1,n+1):
    print(a,end=" ")
    a=a*3
    
 #Series 4
 n=int(input())
