#Hello World
print("Hello World")


#Hello World with tab
print("hello world\thello world")


#Hello World with line
print("Hello World\nHello World")


#Integer Input
a=int(input())
print(a)


#Float Input
a=float(input())
print("%.2f"%a)


#Character Input
a=input()
print(a)


#String Input
a=input()
print(a)


3Ascii 1
a=input()
print(ord(a))


#Ascii 2
a=int(input())
print(chr(a))
