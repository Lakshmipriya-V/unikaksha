#Budget
A=int(input())
B=int(input())
C=int(input())
D=int(input())
E=int(input())
cost=(A*350.34)+(B*230.90)+(C*190.55)+(D*125.30)+(E*180.90)
if cost<=15000:
    print("Yes")
else:
    print("No")
    
    
#Sece Dining
a=input()
b=int(input())
if a=='front' and b==1:
    print("Left Handed")
elif a=='front' and b==2:
    print("Right Handed")
elif a=='rear' and b==1:
    print("Right Handed")
elif a=='rear' and b==2:
    print("Left Handed")
else:
    print("invalid input")
    
    
#Intersection of a circle
x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
c1c2=((x1-x2)**2+(y1-y2)**2)**0.5
if(c1c2==r1+r2):
    print("the circles are tangential")
elif(c1c2<r1+r2):
    print("the circles overlap")
else:
    print("the circles are not overlap")
    
    
#Microwave Oven
print("Enter the number of items")
a=int(input())
print("Enter the single item heating time")
b=float(input())
if a<2:
    print("The recommended heating time is %f "%b)
elif a==2:
    print("The recommended heating time is %.2f"%(b+50/100*b))
elif a==3:
    print("The recommended heating time is %.f"%(2*b))
elif a>3:
    print("Number of items is more")
    
    
